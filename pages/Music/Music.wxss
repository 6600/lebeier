.Music-List {
  position: relative;
  overflow: hidden;
  width: 100%;
  
} 
.Music-box {
  top: 10px;
  width: 100%;
  height: 400px;
  animation: rotar 15s infinite linear;
  transform-style: preserve-3d;
}
.Music-List .Song {
  height: 110rpx;
  text-align: center;
  line-height: 110rpx;
  overflow: hidden;
	position: absolute;
  top:0;
  right:0;
  margin:auto;
  bottom:0;
  left:0;
} 
.Music-List .Song .Music-logo {
  float: left;
  width: 150rpx;
  height: 110rpx;
  margin-right: 10rpx;
  background-color: red;
  border-radius:4rpx;
}
.Music-List .Song .Song-name {
  float: right;
  width: 440rpx;
  height: 110rpx;
  background-color: pink;
  border-radius:4rpx;
}
