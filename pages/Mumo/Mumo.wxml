<!-- 音乐播放模板 -->
<import src ="../template/template.wxml"/>
 <template is="switch"  data="{{flag}}"/>
 <view class='Crumbs'>
  {{Crumbs}}
</view>
<view class="stage {{flag ? '' : 'open'}}">
  <view class='stage-class'>
    <view class='stage-content ' bindtap='JumpProduct'>
      <image src='/image/index/xiaoban.png'></image>
    </view>
     <view class='stage-content'>
       <image src='/image/index/zhongban.png'></image>
    </view>
     <view class='stage-content'>
       <image src='/image/index/daban.png'></image>
    </view>

  </view>
      
</view>
<!-- 播放器 -->
<template is="Playback" data="{{isPlaying, totalProcess, sliderValue}}"/>