.stage {
  width:100%;
  height: 100%;
  background-color:white;
}
.stage-class {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  width: 580rpx;
  height: 810rpx;
}
.stage-class .stage-content {
  margin-bottom: 30rpx; 
  text-align: center;
  line-height: 30rpx;
}
.stage-content>image{
  width: 580rpx;
  height:240rpx; 
}
/* 面包屑 */
.Crumbs {
  float:right;
  font-size: 28rpx;
  margin-top: 30rpx;
  margin-right: 50rpx;
}
